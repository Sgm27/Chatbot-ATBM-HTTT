  <data key="d4">unknown_source</data>
  <data key="d5">1747627261</data>
</node>
<node id="Eulers totient φ(n)">
  <data key="d0">Eulers totient φ(n)</data>
  <data key="d1">category</data>
  <data key="d2">Value computed as (p−1) × (q−1) during RSA key generation; used to determine valid exponents for encryption and decryption.</data>
